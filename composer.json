{
    "name": "k9u/php-skeleton",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuu Kawanami",
            "email": "yuu.kawanami@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "K9u\\PhpSkeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "K9u\\PhpSkeleton\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
